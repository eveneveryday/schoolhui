/* 导入图标库 */
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}


.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconxinghao:before {
  content: "\e610";
}

.icondianhua:before {
  content: "\e69a";
}

.iconweixin:before {
  content: "\e6d9";
}

.iconfanhui:before {
  content: "\e643";
}

.iconfanhui2:before {
  content: "\e601";
}

.iconfanhui1:before {
  content: "\e600";
}






.container{
  background-color:#eee;
  /* padding-left:20rpx;
  padding-right:20rpx; */
  min-height:1200rpx;
}
/* header */
.header_wrap{
  height:80rpx;
  background-color:#fff;
  padding-left:20rpx;
  padding-right:20rpx;
  margin-bottom:50rpx;
}
.header{
  height:80rpx;
  line-height:80rpx;
  background-color:#fff;
  display:flex;
  
}
.iconfont{
  color:#444;
  font-weight:900;
}
.header span{
  font-size:35rpx;
  font-weight:600;
  margin:0 auto;
}

/* 用户信息列表项 */
.list_wrap{
  background-color:#fff;
  padding-left:35rpx;
}
/* .main{
  display:flex;
  flex-direction: column;
  align-items:center;
  justify-content:space-between;
  box-sizing: border-box;  
} */
.lists{
  height:100rpx;
  line-height:100rpx;
  padding-right:20rpx;
  border-bottom:1px solid #ddd;
}
.photo{
  display:flex;
  justify-content:space-between;
}
.tphoto{
  font-size:32rpx;
  color:#444;
  font-weight:600;
}
.touxiang{
  width:85rpx;
  height:85rpx;
  border-radius:50%;
}
.image{
  display:flex;
  justify-content:center;
  align-items:center;
  
}
.getinfo{
  color:#999;
 
}
.fanhui2{
  display:inline-block;
  margin-left:30rpx;
  color:#bbb;
  margin-top:10rpx;
}
.hidden{
  visibility: hidden;
}

/* 退出 */

.list{
  height:250rpx;
  background-color:#fff;
  display:flex;
  justify-content:center;
  align-items: flex-end;
  padding-bottom:35rpx;
}
.exit{
  color:firebrick;
  font-size:35rpx;
  display:block;
}
.main .list2{
  margin-top:30rpx;
}
.odate{
  border:1px solid rgb(230, 40, 40);
  display:inline-block;
}

